// grab countries

// ====================== COUNTRIES =================================

data=[]; links.forEach(l => {
  const rx = /flag-([a-z]+)"/i
  data.push({
    name: l.innerText.replace('\n', ''),
    link: l.href,
    countryCode: rx.exec(l.innerHTML)[1]
  }) 
}); console.log(JSON.stringify(data))
